{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"],
      "dest": "index.js"
    }
  ]
}
{
  "headers": [
    {
      "source": "/events",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://backend-integration-bi-qdm6-d5smbjry4.vercel.app"
        }
      ]
    }
  ]
}
